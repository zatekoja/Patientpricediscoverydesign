# Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# API Base URL (optional, defaults to /api)
# VITE_API_BASE_URL=http://localhost:8080/api
# SSE Base URL (optional, defaults to VITE_API_BASE_URL)
# VITE_SSE_BASE_URL=http://localhost:8082/api

# Google Maps API Key (optional, app will use static maps as fallback)
# Get your key from: https://console.cloud.google.com/apis/credentials
# VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Vault (dev defaults)
VAULT_ENABLED=true
VAULT_ADDR=http://localhost:8200
VAULT_TOKEN=root
VAULT_NAMESPACE=
VAULT_MOUNT=secret
VAULT_API_PATH=patient-price-discovery/api
VAULT_PROVIDER_PATH=patient-price-discovery/provider
VAULT_KV_VERSION=2
VAULT_TIMEOUT_MS=5000
VAULT_OVERWRITE=false
